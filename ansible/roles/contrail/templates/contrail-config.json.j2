{
    "command": "/usr/bin/supervisord --nodaemon -c /etc/contrail/supervisord_config.conf",
    "config_files": [
        {
            "source": "{{ container_config_directory }}/contrail-config-nodemgr.conf",
            "dest": "/etc/contrail/contrail-config-nodemgr.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "{{ container_config_directory }}/contrail-api.conf",
            "dest": "/etc/contrail/contrail-api.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "{{ container_config_directory }}/contrail-device-manager.conf",
            "dest": "/etc/contrail/contrail-device-manager.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "{{ container_config_directory }}/contrail-discovery.conf",
            "dest": "/etc/contrail/contrail-discovery.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "{{ container_config_directory }}/contrail-schema.conf",
            "dest": "/etc/contrail/contrail-schema.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "{{ container_config_directory }}/contrail-keystone-auth.conf",
            "dest": "/etc/contrail/contrail-keystone-auth.conf",
            "owner": "root",
