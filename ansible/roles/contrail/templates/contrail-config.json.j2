            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "{{ container_config_directory }}/publisher.properties",
            "dest": "/etc/ifmap-server/publisher.properties",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "{{ container_config_directory }}/contrail-svc-monitor.conf",
            "dest": "/etc/contrail/contrail-svc-monitor.conf",
            "owner": "root",
            "perm": "0644"
