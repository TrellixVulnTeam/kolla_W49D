{
    "command": "/usr/bin/supervisord --nodaemon -c /etc/contrail/supervisord_analytics.conf",
    "config_files": [
        {
            "source": "{{ container_config_directory }}/contrail-query-engine.conf",
            "dest": "/etc/contrail/contrail-query-engine.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "{{ container_config_directory }}/contrail-topology.conf",
            "dest": "/etc/contrail/contrail-topology.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "{{ container_config_directory }}/contrail-analytics-nodemgr.conf",
            "dest": "/etc/contrail/contrail-analytics-nodemgr.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "{{ container_config_directory }}/contrail-collector.conf",
            "dest": "/etc/contrail/contrail-collector.conf",
            "owner": "root",
            "perm": "0644"
